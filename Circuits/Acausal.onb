    <Text>&lt;html>&lt;head>&lt;meta name="qrichtext" content="1" />&lt;/head>&lt;body style=" white-space: pre-wrap; font-family:MS Shell Dlg; font-size:8.25pt; font-weight:400; font-style:normal; text-decoration:none;">&lt;p style=" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px; font-family:Arial; font-size:38pt; font-weight:600; color:#000000;">Acausal Physical Modeling - Simple Circuit&lt;/p>&lt;/body>&lt;/html></Text>
   </TextCell>
   <TextCell style="Title" >
    <Text>&lt;html>&lt;head>&lt;meta name="qrichtext" content="1" />&lt;/head>&lt;body style=" white-space: pre-wrap; font-family:MS Shell Dlg; font-size:8.25pt; font-weight:400; font-style:normal; text-decoration:none;">&lt;p style=" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px; font-family:Arial; font-size:38pt; font-weight:600; color:#000000;">&lt;span style=" font-weight:400; color:#000000;">&lt;img src="file:///C:/OpenModelica1.4.2/bin/OMNoteboook_tempfiles/23.png" width="407" height="318" />&lt;/span>&lt;/p>&lt;/body>&lt;/html></Text>
    <Image name="file:///C:/OpenModelica1.4.2/bin/OMNoteboook_tempfiles/23.png" ></Image>
   </TextCell>
   <TextCell style="Text" >
    <Text>&lt;html>&lt;head>&lt;meta name="qrichtext" content="1" />&lt;/head>&lt;body style=" white-space: pre-wrap; font-family:MS Shell Dlg; font-size:8.25pt; font-weight:400; font-style:normal; text-decoration:none;">&lt;p align='center' style=" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px; font-family:Times New Roman; font-size:12pt; color:#000000;">&lt;span style=" font-weight:600;">Diagram 1:&lt;/span>  Connection diagram of the acausal simple circuit  model.&lt;/p>&lt;/body>&lt;/html></Text>
    <Rule name="TextAlignment" >Center</Rule>
   </TextCell>
   <TextCell style="Text" >
    <Text>&lt;html>&lt;head>&lt;meta name="qrichtext" content="1" />&lt;/head>&lt;body style=" white-space: pre-wrap; font-family:MS Shell Dlg; font-size:8.25pt; font-weight:400; font-style:normal; text-decoration:none;">&lt;p align='center' style=" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px; font-family:Times New Roman; font-size:12pt; color:#000000;">&lt;br />&lt;/p>&lt;/body>&lt;/html></Text>
    <Rule name="TextAlignment" >Center</Rule>
    <Rule name="CellTags" >Model1</Rule>
   </TextCell>
   <InputCell style="Input" closed="true" >
    <Input>loadModel(Modelica)  // Load if not already loaded</Input>
    <Output></Output>
   </InputCell>
   <InputCell style="Input" closed="false" >
    <Input>model SimpleCircuit
  Modelica.Electrical.Analog.Basic.Resistor Resistor1;
  Modelica.Electrical.Analog.Basic.Inductor Inductor1;
  Modelica.Electrical.Analog.Basic.Ground Ground1;
  Modelica.Electrical.Analog.Basic.Resistor Resistor2;
  Modelica.Electrical.Analog.Basic.Capacitor Capacitor1;
  Modelica.Electrical.Analog.Sources.SineVoltage SineVoltage1;
  
equation
  connect(SineVoltage1.p, Resistor2.p);
  connect(Resistor1.n, Inductor1.p);
  connect(Resistor2.n, Capacitor1.p);
  connect(Inductor1.n, Ground1.p);
  connect(Capacitor1.n, Ground1.p);
  connect(Resistor1.p, Resistor2.p);
  connect(SineVoltage1.n, Ground1.p); 
end SimpleCircuit;  </Input>
    <Output>Ok</Output>
   </InputCell>
   <TextCell style="" >
    <Text>&lt;html>&lt;head>&lt;meta name="qrichtext" content="1" />&lt;/head>&lt;body style=" white-space: pre-wrap; font-family:MS Shell Dlg; font-size:8.25pt; font-weight:400; font-style:normal; text-decoration:none;">&lt;p style=" margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px; font-size:8pt;">&lt;span style=" font-family:Times New Roman; font-size:12pt; color:#000000;">  &lt;/span>&lt;/p>&lt;/body>&lt;/html></Text>
   </TextCell>
  </GroupCell>
  <TextCell style="Text" >
   <Text>&lt;html>&lt;head>&lt;meta name="qrichtext" content="1" />&lt;/head>&lt;body style=" white-space: pre-wrap; font-family:MS Shell Dlg; font-size:8.25pt; font-weight:400; font-style:normal; text-decoration:none;">&lt;p style="-qt-paragraph-type:empty; margin-top:0px; margin-bottom:0px; margin-left:0px; margin-right:0px; -qt-block-indent:0; text-indent:0px; font-family:Times New Roman; font-size:12pt; color:#000000;">&lt;/p>&lt;/body>&lt;/html></Text>
  </TextCell>
  <InputCell style="Input" closed="false" >
   <Input>instantiateModel(SimpleCircuit)  // Take a look at the flattened model - not needed  ??Error</Input>
   <Output>"fclass SimpleCircuit
Real Resistor1.v "Voltage drop between the two pins (= p.v - n.v)";
Real Resistor1.i "Current flowing from pin p to pin n";
Real Resistor1.p.v "Potential at the pin";
Real Resistor1.p.i "Current flowing into the pin";
Real Resistor1.n.v "Potential at the pin";
Real Resistor1.n.i "Current flowing into the pin";
parameter Real Resistor1.R = 1 "Resistance";
Real Inductor1.v "Voltage drop between the two pins (= p.v - n.v)";
Real Inductor1.i "Current flowing from pin p to pin n";
Real Inductor1.p.v "Potential at the pin";
Real Inductor1.p.i "Current flowing into the pin";
Real Inductor1.n.v "Potential at the pin";
Real Inductor1.n.i "Current flowing into the pin";
parameter Real Inductor1.L = 1 "Inductance";
Real Ground1.p.v "Potential at the pin";
Real Ground1.p.i "Current flowing into the pin";
Real Resistor2.v "Voltage drop between the two pins (= p.v - n.v)";
Real Resistor2.i "Current flowing from pin p to pin n";
Real Resistor2.p.v "Potential at the pin";
Real Resistor2.p.i "Current flowing into the pin";
Real Resistor2.n.v "Potential at the pin";
Real Resistor2.n.i "Current flowing into the pin";
parameter Real Resistor2.R = 1 "Resistance";
Real Capacitor1.v "Voltage drop between the two pins (= p.v - n.v)";
Real Capacitor1.i "Current flowing from pin p to pin n";
Real Capacitor1.p.v "Potential at the pin";
Real Capacitor1.p.i "Current flowing into the pin";
Real Capacitor1.n.v "Potential at the pin";
Real Capacitor1.n.i "Current flowing into the pin";
parameter Real Capacitor1.C = 1 "Capacitance";
Real SineVoltage1.v "Voltage drop between the two pins (= p.v - n.v)";
Real SineVoltage1.i "Current flowing from pin p to pin n";
Real SineVoltage1.p.v "Potential at the pin";
Real SineVoltage1.p.i "Current flowing into the pin";
Real SineVoltage1.n.v "Potential at the pin";
Real SineVoltage1.n.i "Current flowing into the pin";
parameter Real SineVoltage1.offset = 0 "Voltage offset";
parameter Real SineVoltage1.startTime = 0 "Time offset";
Real SineVoltage1.signalSource.y "Connector of Real output signal";
parameter Real SineVoltage1.signalSource.amplitude = SineVoltage1.V "Amplitude of sine wave";
parameter Real SineVoltage1.signalSource.freqHz = SineVoltage1.freqHz "Frequency of sine wave";
parameter Real SineVoltage1.signalSource.phase = SineVoltage1.phase "Phase of sine wave";
parameter Real SineVoltage1.signalSource.offset = SineVoltage1.offset "Offset of output signal";
parameter Real SineVoltage1.signalSource.startTime = SineVoltage1.startTime "Output = offset for time &lt; startTime";
constant Real SineVoltage1.signalSource.pi = 3.14159265358979;
parameter Real SineVoltage1.V = 1 "Amplitude of sine wave";
parameter Real SineVoltage1.phase = 0 "Phase of sine wave";
parameter Real SineVoltage1.freqHz = 1 "Frequency of sine wave";
equation
  Resistor1.R * Resistor1.i = Resistor1.v;
  Resistor1.v = Resistor1.p.v - Resistor1.n.v;
  0.0 = Resistor1.p.i + Resistor1.n.i;
  Resistor1.i = Resistor1.p.i;
  Inductor1.L * der(Inductor1.i) = Inductor1.v;
  Inductor1.v = Inductor1.p.v - Inductor1.n.v;
  0.0 = Inductor1.p.i + Inductor1.n.i;
  Inductor1.i = Inductor1.p.i;
  Ground1.p.v = 0.0;
  Resistor2.R * Resistor2.i = Resistor2.v;
  Resistor2.v = Resistor2.p.v - Resistor2.n.v;
  0.0 = Resistor2.p.i + Resistor2.n.i;
  Resistor2.i = Resistor2.p.i;
  Capacitor1.i = Capacitor1.C * der(Capacitor1.v);
  Capacitor1.v = Capacitor1.p.v - Capacitor1.n.v;
  0.0 = Capacitor1.p.i + Capacitor1.n.i;
  Capacitor1.i = Capacitor1.p.i;
  SineVoltage1.signalSource.y = SineVoltage1.signalSource.offset + if time &lt; SineVoltage1.signalSource.startTime then 0.0 else SineVoltage1.signalSource.amplitude * Modelica.Math.sin(6.28318530717959 * SineVoltage1.signalSource.freqHz * (time - SineVoltage1.signalSource.startTime) + SineVoltage1.signalSource.phase);
  SineVoltage1.v = SineVoltage1.signalSource.y;
  SineVoltage1.v = SineVoltage1.p.v - SineVoltage1.n.v;
  0.0 = SineVoltage1.p.i + SineVoltage1.n.i;
  SineVoltage1.i = SineVoltage1.p.i;
  SineVoltage1.n.i + Capacitor1.n.i + Inductor1.n.i + Ground1.p.i = 0.0;
  SineVoltage1.n.v = Capacitor1.n.v;
  Capacitor1.n.v = Inductor1.n.v;
  Inductor1.n.v = Ground1.p.v;
  Resistor1.p.i + SineVoltage1.p.i + Resistor2.p.i = 0.0;
  Resistor1.p.v = SineVoltage1.p.v;
  SineVoltage1.p.v = Resistor2.p.v;
  Resistor2.n.i + Capacitor1.p.i = 0.0;
  Resistor2.n.v = Capacitor1.p.v;
  Resistor1.n.i + Inductor1.p.i = 0.0;
  Resistor1.n.v = Inductor1.p.v;
end SimpleCircuit;
"
