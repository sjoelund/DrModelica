  Integer    arr3d[n,m,k] = fill(1,n,m,k);
  Boolean    truthvalues[2]     = {false, true};
  Voltage    voltvector[10] = fill(10, 10);
end ArrayDim2;</Input>
          <Output>{ArrayDim2}</Output>
        </GraphCell>
