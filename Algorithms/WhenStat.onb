        </GraphCell>
        <GraphCell AutoX="true" AutoY="true" style="Graph" MinorX="0.1" Antialiasing="false" MinorY="0.05" Title="Plot by OpenModelica" LogX="false" LogY="false" MajorX="0.2" MajorY="0.1" closed="false" YLabel="" Area="v2P3VpTKvqG/h38OMD6FDD/wVulaw+sAP+5ZclOmFoQ=" ShowGraph="true" Grid="true" XLabel="time" Legend="true" >
          <Data Id="0" Label="time" ></Data>
